// A launch configuration that compiles the extension and then opens it inside a new window
{
    "version": "0.2.0",
    "configurations": [

        {
            "name": "Launch Extension Typescript",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "${workspaceRoot}/test/typeScript-test"
            ],
            "stopOnEntry": false,
            "env": {
                "MOCHA_DEBUG": "false"
            }
        },
        {
            "name": "Launch Extension babel",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                " /home/matyz/dev/open-source/grabage/babel"
            ],
            "stopOnEntry": false,
            "env": {
                "MOCHA_DEBUG": "false"
            }
        },
        {
            "name": "Launch Extension",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "${workspaceRoot}/test/workspace"
            ],
            "stopOnEntry": false,
            "env": {
                "MOCHA_DEBUG": "false"
            }
        },
        {
            "name": "Launch worker",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/yehil/dev/hkube/worker/"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch etcd",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/hkube-common/etcd.hkube/"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch caching",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/openSource/cashing-service"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch nooow",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "C:\\dev\\noooow"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch es6",
            "type": "extensionHost",
            "request": "launch",
            "env": {
                "MOCHA_DEBUG": "true"
            },
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "C:\\dev\\es6-mocha"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch gateway",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/gateway"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch producer",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "/home/matyz/dev/hkube-common/producer/"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Launch Tests",
            "type": "extensionHost",
            "request": "launch",
            "runtimeExecutable": "${execPath}",
            "args": [
                "--extensionDevelopmentPath=${workspaceRoot}",
                "--extensionTestsPath=${workspaceRoot}/test"
            ],
            "stopOnEntry": false
        },
        {
            "name": "Attach child",
            "type": "node",
            "request": "attach",
            "port": 5859,
            "address": "localhost",
            "restart": false,
            "sourceMaps": false,
            "outFiles": [],
            "localRoot": "${workspaceRoot}",
            "remoteRoot": null,
            "protocol": "inspector"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch recurse",
            "program": "${workspaceRoot}/stub/recursion"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch stub",
            "program": "${workspaceRoot}/inProcess/stub"
        }
    ]
}